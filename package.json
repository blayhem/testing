{
  "name": "sigma-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@zeit/next-sass": "^1.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "next": "9.5.5",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "^7.2.1",
    "sass": "^1.27.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(scss|less)$": "identity-obj-proxy",
      "components/(.*)": "<rootDir>/components/$1",
      "styles/(.*)": "<rootDir>/styles/$1"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/components/setupTests.js"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "babel-jest": "^26.5.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.3",
    "react-test-renderer": "^16.14.0"
  }
}
